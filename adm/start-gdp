#!/bin/sh 
export HOME=`echo ~gdp`
cd $HOME

if [ -x bin/run-gdp-router ]
then
	bin/run-gdp-router &
	sleep 2
fi
if [ -x bin/run-gdplogd ]
then
	bin/run-gdplogd &
	sleep 2
fi
if [ -x bin/run-gdp-rest ]
then
	bin/run-gdp-rest &
	sleep 2
fi
if [ -x bin/run-gdp-machine-mon ]
then
	bin/run-gdp-machine-mon &
	sleep 2
fi
